{
  "name": "webui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "ca": "cd ../server/ca-server && ./ca",
    "ca-web": "cd ../ && ./ca_web",
